{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "max-line-length": [true, 160],
        "trailing-comma": [false],
        "object-literal-sort-keys": [  false         ],
        "semicolon": [  true, "never" ],
        "jsx-no-multiline-js": false,
        "jsx-wrap-multiline": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": []
}
